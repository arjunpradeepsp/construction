    function firePixel(url) {
      if (url && url.length) {
        new Image().src = url;
      }
    }

    function handleThirdCreative() {
      var query = triggerThirdPixelMediaQuery[creativeType];
      if (!window.matchMedia || !pixels.third || !pixels.third.length || !query) return;

