      var P13N_FORESTER_CHANNEL_NAME = 'p13n-shared-components';
      var PERCOLATE_IMP_DATA = 'action/sb_detail:ad1,ad2:action=view,pd_rd_w=Wh0PB,pf_rd_p=efe0582f-a424-4ac7-a014-0a26fc9c0a9b,pd_rd_wg=antD1,pf_rd_r=100EF95YQT3GME8SFFH3,pd_rd_r=7f4a62b3-f03b-43e5-839f-e2772e27c1f7?marketplaceId=A21TJRUUN4KGV&requestId=7f4a62b3-f03b-43e5-839f-e2772e27c1f7&session=262-5874428-4054109';

